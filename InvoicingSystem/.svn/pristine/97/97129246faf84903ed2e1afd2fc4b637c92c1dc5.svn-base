package vo;

public class TotalCouponVO extends CouponVO{
	
	private double totalMoney;
	
	public TotalCouponVO(double totalMoney,int price,int num,String endDate){
		this.totalMoney=totalMoney;
		this.price=price;
		this.num=num;
		this.endDate=endDate;
	}
	
	public double getTotalMoney(){
		return this.totalMoney;
	}
	
	public void add(int n){
		num=num+n;
	}
}
