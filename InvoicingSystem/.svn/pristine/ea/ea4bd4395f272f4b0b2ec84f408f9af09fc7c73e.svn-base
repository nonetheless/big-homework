package vo;

public class TotalPromotionVO extends PromotionVO{
	
	private double totalPrice;
	
	public TotalPromotionVO(double totalPrice){
		this.totalPrice=totalPrice;
		this.isGift=false;
		this.isCoupon=false;
	}
	
	public TotalPromotionVO(double totalPrice,boolean isCoupon,boolean isGift){
		this.totalPrice=totalPrice;
		this.isCoupon=isCoupon;
		this.isGift=isGift;
	}
	
	public double getTotalPrice(){
		return this.totalPrice;
	}
	
}
