package vo;

public class goodsVO {
	String goodkind;
	String goodname;
	String goodtype;
	double inprice;
	double outprice;
    double recentin;
    double recentout;
    public goodsVO(String s1,String s2,String s3,double d1,double d2,double d3,double d4){
    	goodkind=s1;
    	goodname=s2;
    	goodtype=s3;
    	inprice=d1;
    	outprice=d2;
        recentin=d3;
        recentout=d4;
    }
    public String get(){
    	StringBuilder builder=new StringBuilder();
    	builder.append(goodkind);
    	builder.append(goodname);
    	builder.append(goodtype);
    	builder.append(inprice);
    	builder.append(outprice);
    	builder.append(recentin);
    	builder.append(recentout);
    	String s=builder.toString();
    	return s;
    }

}
