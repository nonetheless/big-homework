package vo;

public class accountlineitemVO {
	String name;
	double money;
	String remark;
	public accountlineitemVO(String s1,double d,String s2){
		name=s1;
		money=d;
		remark=s2;
	}
	public String  getname(){
		return name;
	}
	public double  getmoney(){
		return money;
	}
	public String  getremark(){
		return remark;
	}

}
