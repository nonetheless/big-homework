package presentation.kindui;

import javax.swing.JButton;
import javax.swing.JDialog;
import javax.swing.JTextField;

public class FieldDialog extends JDialog {

	JTextField field;
	JButton button;
	
	
	public FieldDialog(KindUI ui){
		setSize(200,200);
		setLocation(500,500);
	    
		field=new JTextField(20);
		button=new JButton("чбошод");
		
		
		add(field);
		add(button);
		
		
	}
	public static void main(String args[]){
		FieldDialog fd=new FieldDialog(new KindUI());
	}
	
}
